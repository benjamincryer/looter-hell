for(i=0;i&lt;stages[area];i++) {
    if(point_in_rectangle(mouse_x,mouse_y,xp,yp+(i*16),xp+16,yp+(i*16+16))) {
    sel = i;
    moused = true;
    }
}

if (sel&gt;stages[area]-1) sel = stages[area]-1;
if (sel&lt;0) sel = 0;

//select option
if (keyboard_check_pressed(vk_enter) or (moused == true and mouse_check_button_pressed(mb_left))) {
    audio_play_sound(sfxMenuSelect,10,false);
    global.area = area;
    global.stage = sel;
    
    room_goto(rmBattle);
}
