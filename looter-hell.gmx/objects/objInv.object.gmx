//BACKGROUND
c = merge_color(c_white,c_dkgray,0.5);
draw_rectangle_colour(xp-8,yp-8,xp+32+loadwidth+invwidth,yp+32+(scrInvSize()*16),c_white,c_white,c,c,0);
