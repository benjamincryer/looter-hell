    if (instance_exists(objEnemy)) {
    target = instance_nearest(x,y,objEnemy);
    origspeed = speed;
    speed = 0;
    direction = 0;
    bloodhound = 2;
    }
    
    //reset speed if all enemies dead
    if (speed == 0) speed = origspeed;
}

//Bloodhound tracking
if (bloodhound == 2) {
    if (instance_exists(target)) move_towards_point(target.x,target.y,origspeed);
    else bloodhound = 1;
}

//Zigzag activate
if (zigzag == 1) { alarm[4] = timetrans; zigzag = 2; }

//Lock activate
if (lock == 1) {
    target = instance_nearest(x,y,objEnemy);
    
    if (target != noone) {
    varx = round(random_range(-10,10)); vary = round(random_range(-10,10)); //add variation
    direction = point_direction(x,y,target.x+varx,target.y+vary);
    lock = 2;
    }
}

//Change direction if set to curve
if (curve != 0) {
    direction += curve; //otherwise curve
}

//Swirling bullets
if (vortex == 1) {
    //move in current direction
    if (dir == 0) direction += speed; else direction -= speed;
    
    //change direction
    if (direction &gt;= 45) dir = 1; else if (direction &lt;= -45) dir = 0;
}

