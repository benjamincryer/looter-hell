        draw_text(x+4,y+gheight,string(element));
        draw_text(x+4,y+gheight+10,load_text("elementchance") + ": " + string(elem_chance));
    }
    //DPS
    draw_text(x+5,y,"DPS: " + string(dps));
    
    //draw stats
    draw_text(x+width1,y,string(prefix) + " " + string(name));
    draw_text(x+width1,y+10,"Level " + string(level) + " " + string(type));
    
    if (bnum &gt; 1) draw_text(x+width1,y+20,"Damage: " + string(damage) + "x" + string(bnum));
    else draw_text(x+width1,y+20,"Damage: " + string(damage));
    
    draw_text(x+width1,y+30,"Accuracy: " + string(acc));
